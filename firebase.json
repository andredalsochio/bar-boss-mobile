{
  "projects": {
    "default": "bar-boss-mobile"
  },
  "targets": {
    "bar-boss-mobile": {
      "hosting": {
        "production": ["bar-boss-mobile"]
      }
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "bar-boss-mobile.appspot.com"
  },
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  },
  "crashlytics": {
    "symbolUploadEnabled": true
  }
}